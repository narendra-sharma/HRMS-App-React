{
  "expo": {
    "name": "HRPMS",
    "slug": "hrms",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/hrmsLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#055C9D"
    },
    "assetBundlePatterns": ["**/*"],
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manyavv.hrms",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyAzXDEebJV9MxtPAPhP1B2w5T3AYK2JOu0"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/hrmsLogo.png",
        "backgroundColor": "#055C9D"
      },
      "package": "com.manyavv.hrms",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAzXDEebJV9MxtPAPhP1B2w5T3AYK2JOu0"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0e22c036-9a1e-436e-b4c3-6aa349b33fcf"
      }
    },
    "owner": "ajay2305"
  }
}
